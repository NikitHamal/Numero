    <!-- About -->
    <string name="about">About</string>
    <string name="about_description">Numero is a comprehensive numerology app that provides detailed analysis of your core numbers, life cycles, and compatibility with others. Using both Pythagorean and Chaldean systems, it offers accurate insights into your life path and personal characteristics.</string>
    <string name="features">Features</string>
    <string name="feature_1">Complete numerology profile analysis</string>
    <string name="feature_2">Life Path, Expression, Soul Urge calculations</string>
    <string name="feature_3">Pinnacles, Challenges, and Life Periods</string>
    <string name="feature_4">Compatibility analysis between profiles</string>
    <string name="feature_5">Personal and Universal daily cycles</string>
    <string name="feature_6">Support for English and Nepali</string>
    <string name="calculation_systems">Calculation Systems</string>
    <string name="pythagorean_info">Pythagorean: The most widely used Western system where letters A-Z are assigned numbers 1-9 based on their position in the alphabet.</string>
    <string name="chaldean_info">Chaldean: An ancient Babylonian system where letters are assigned numbers 1-8 based on their sound vibration. Number 9 is considered sacred.</string>
    <string name="open_source">Open Source Application</string>

    <!-- Life Path Number Interpretations -->
    <string name="life_path_1_title">The Leader</string>
    <string name="life_path_1_meaning">Life Path 1 represents the path of the pioneer and innovator. You are here to develop freedom, individuality, and confidence. Your journey involves becoming self-reliant, initiating projects, and leading others by your example.</string>
    <string name="life_path_1_strength_1">Natural leadership abilities</string>
    <string name="life_path_1_strength_2">Strong determination and willpower</string>
    <string name="life_path_1_strength_3">Original and creative thinking</string>
    <string name="life_path_1_strength_4">Self-motivation and drive</string>
    <string name="life_path_1_strength_5">Courage to take initiative</string>
    <string name="life_path_1_challenge_1">Tendency toward stubbornness</string>
    <string name="life_path_1_challenge_2">May struggle with collaboration</string>
    <string name="life_path_1_challenge_3">Risk of becoming domineering</string>
    <string name="life_path_1_challenge_4">Impatient with others</string>
    <string name="life_path_1_challenge_5">Fear of failure or dependence</string>
    <string name="life_path_1_career_1">Entrepreneur</string>
    <string name="life_path_1_career_2">Executive</string>
    <string name="life_path_1_career_3">Inventor</string>
    <string name="life_path_1_career_4">Director</string>
    <string name="life_path_1_career_5">Freelancer</string>
    <string name="life_path_1_career_6">Military Leader</string>
    <string name="life_path_1_relationship">In relationships, you need a partner who respects your independence and keeps your energy in check. Avoid controlling behavior and learn to compromise without losing yourself.</string>
    <string name="life_path_1_spiritual">Your spiritual lesson is to balance determination with humility. Learn that true leadership comes from serving others, not dominating them.</string>
    <string name="life_path_1_colors">Red, Gold, Yellow</string>
    <string name="life_path_1_days">Sunday, Monday</string>

    <string name="life_path_2_title">The Diplomat</string>
    <string name="life_path_2_meaning">Life Path 2 represents the path of cooperation and harmony. You are here to bring peace, balance, and partnership into the world. Your sensitivity and intuition make you an excellent mediator and supporter.</string>
    <string name="life_path_2_strength_1">Deep intuition and sensitivity</string>
    <string name="life_path_2_strength_2">Natural peacemaking abilities</string>
    <string name="life_path_2_strength_3">Cooperative and diplomatic nature</string>
    <string name="life_path_2_strength_4">Patience and understanding</string>
    <string name="life_path_2_strength_5">Strong sense of justice</string>
    <string name="life_path_2_challenge_1">Extreme sensitivity to criticism</string>
    <string name="life_path_2_challenge_2">Tendency toward indecisiveness</string>
    <string name="life_path_2_challenge_3">Can become overly dependent</string>
    <string name="life_path_2_challenge_4">Difficulty being assertive</string>
    <string name="life_path_2_challenge_5">Prone to mood swings</string>
    <string name="life_path_2_career_1">Counselor</string>
    <string name="life_path_2_career_2">Mediator</string>
    <string name="life_path_2_career_3">Diplomat</string>
    <string name="life_path_2_career_4">Social Worker</string>
    <string name="life_path_2_career_5">Artist</string>
    <string name="life_path_2_career_6">Healthcare Professional</string>
    <string name="life_path_2_relationship">Partnership is essential to your happiness. You thrive in committed relationships with mutual respect and emotional support. Avoid losing yourself in your partner\'s needs.</string>
    <string name="life_path_2_spiritual">Your spiritual path involves developing inner strength while trusting your intuition. Balance serving others with honoring your own needs.</string>
    <string name="life_path_2_colors">White, Cream, Light Green</string>
    <string name="life_path_2_days">Monday, Friday</string>

    <string name="life_path_3_title">The Communicator</string>
    <string name="life_path_3_meaning">Life Path 3 represents the path of creative expression and joy. You are here to inspire others through your words, art, and infectious optimism. Communication in all forms is your gift to the world.</string>
    <string name="life_path_3_strength_1">Excellent communication skills</string>
    <string name="life_path_3_strength_2">Natural creativity and artistry</string>
    <string name="life_path_3_strength_3">Infectious enthusiasm</string>
    <string name="life_path_3_strength_4">Social charm and charisma</string>
    <string name="life_path_3_strength_5">Uplifting and inspiring presence</string>
    <string name="life_path_3_challenge_1">Tendency to scatter energy</string>
    <string name="life_path_3_challenge_2">May avoid emotional depth</string>
    <string name="life_path_3_challenge_3">Risk of superficiality</string>
    <string name="life_path_3_challenge_4">Difficulty with focus</string>
    <string name="life_path_3_challenge_5">Prone to exaggeration or gossip</string>
    <string name="life_path_3_career_1">Writer</string>
    <string name="life_path_3_career_2">Actor</string>
    <string name="life_path_3_career_3">Musician</string>
    <string name="life_path_3_career_4">Public Speaker</string>
    <string name="life_path_3_career_5">Marketing Professional</string>
    <string name="life_path_3_career_6">Designer</string>
    <string name="life_path_3_relationship">You bring joy and laughter to relationships but may struggle with emotional depth. Find a partner who appreciates your creativity while helping you stay grounded.</string>
    <string name="life_path_3_spiritual">Your spiritual journey involves learning that true creativity comes from emotional authenticity. Express the full range of human experience, not just joy.</string>
    <string name="life_path_3_colors">Yellow, Orange, Pink</string>
    <string name="life_path_3_days">Wednesday, Thursday</string>

    <string name="life_path_4_title">The Builder</string>
    <string name="life_path_4_meaning">Life Path 4 represents the path of stability and foundation. You are here to create lasting structures, whether physical, organizational, or conceptual. Your reliability and dedication are the backbone of any endeavor.</string>
    <string name="life_path_4_strength_1">Exceptional organizational skills</string>
    <string name="life_path_4_strength_2">Strong work ethic</string>
    <string name="life_path_4_strength_3">Practical and reliable nature</string>
    <string name="life_path_4_strength_4">Attention to detail</string>
    <string name="life_path_4_strength_5">Patience and determination</string>
    <string name="life_path_4_challenge_1">Tendency toward rigidity</string>
    <string name="life_path_4_challenge_2">May resist change</string>
    <string name="life_path_4_challenge_3">Risk of workaholism</string>
    <string name="life_path_4_challenge_4">Difficulty with spontaneity</string>
    <string name="life_path_4_challenge_5">Prone to pessimism</string>
    <string name="life_path_4_career_1">Engineer</string>
    <string name="life_path_4_career_2">Accountant</string>
    <string name="life_path_4_career_3">Project Manager</string>
    <string name="life_path_4_career_4">Architect</string>
    <string name="life_path_4_career_5">Surgeon</string>
    <string name="life_path_4_career_6">Systems Analyst</string>
    <string name="life_path_4_relationship">You provide stability and loyalty in relationships but may struggle with emotional expression. Find a partner who values security while helping you loosen up.</string>
    <string name="life_path_4_spiritual">Your spiritual lesson is to find freedom within structure. True security comes from within, not from external circumstances.</string>
    <string name="life_path_4_colors">Green, Brown, Blue</string>
    <string name="life_path_4_days">Saturday, Sunday</string>

    <string name="life_path_5_title">The Seeker</string>
    <string name="life_path_5_meaning">Life Path 5 represents the path of change and adventure. You are here to experience all the diversity of life, embracing change as your greatest teacher. Your adaptability and curiosity propel you forward.</string>
    <string name="life_path_5_strength_1">Exceptional adaptability</string>
    <string name="life_path_5_strength_2">Natural curiosity</string>
    <string name="life_path_5_strength_3">Dynamic and engaging presence</string>
    <string name="life_path_5_strength_4">Quick thinking</string>
    <string name="life_path_5_strength_5">Resourcefulness</string>
    <string name="life_path_5_challenge_1">Difficulty with commitment</string>
    <string name="life_path_5_challenge_2">Tendency toward excess</string>
    <string name="life_path_5_challenge_3">May avoid responsibility</string>
    <string name="life_path_5_challenge_4">Restlessness and impatience</string>
    <string name="life_path_5_challenge_5">Risk of addiction</string>
    <string name="life_path_5_career_1">Travel Writer</string>
    <string name="life_path_5_career_2">Sales Representative</string>
    <string name="life_path_5_career_3">Journalist</string>
    <string name="life_path_5_career_4">Tour Guide</string>
    <string name="life_path_5_career_5">Event Planner</string>
    <string name="life_path_5_career_6">Consultant</string>
    <string name="life_path_5_relationship">You need freedom in relationships. Find a partner who is independent or adventurous enough to join you on your journey without clinging.</string>
    <string name="life_path_5_spiritual">Your spiritual path involves learning that true freedom is internal. External experiences are teachers, but wisdom comes from inner reflection.</string>
    <string name="life_path_5_colors">Turquoise, Light Blue, Silver</string>
    <string name="life_path_5_days">Wednesday, Friday</string>

    <string name="life_path_6_title">The Nurturer</string>
    <string name="life_path_6_meaning">Life Path 6 represents the path of love and responsibility. You are here to nurture, heal, and create harmony in your environment. Family, community, and service are central to your purpose.</string>
    <string name="life_path_6_strength_1">Deep capacity for love</string>
    <string name="life_path_6_strength_2">Natural healing abilities</string>
    <string name="life_path_6_strength_3">Strong sense of responsibility</string>
    <string name="life_path_6_strength_4">Artistic appreciation</string>
    <string name="life_path_6_strength_5">Protective nature</string>
    <string name="life_path_6_challenge_1">Tendency toward perfectionism</string>
    <string name="life_path_6_challenge_2">Can become controlling</string>
    <string name="life_path_6_challenge_3">Risk of self-sacrifice</string>
    <string name="life_path_6_challenge_4">Difficulty saying no</string>
    <string name="life_path_6_challenge_5">Prone to worry</string>
    <string name="life_path_6_career_1">Teacher</string>
    <string name="life_path_6_career_2">Nurse</string>
    <string name="life_path_6_career_3">Interior Designer</string>
    <string name="life_path_6_career_4">Therapist</string>
    <string name="life_path_6_career_5">Chef</string>
    <string name="life_path_6_career_6">Veterinarian</string>
    <string name="life_path_6_relationship">You are a romantic idealist seeking a beautiful partnership. Be careful not to try to fix or control your partner. Give them their own journey while offering support.</string>
    <string name="life_path_6_spiritual">Your spiritual lesson is to balance giving and receiving. Learn that nurturing yourself allows you to better nurture others.</string>
    <string name="life_path_6_colors">Blue, Pink, Green</string>
    <string name="life_path_6_days">Friday, Thursday</string>

    <string name="life_path_7_title">The Seeker</string>
    <string name="life_path_7_meaning">Life Path 7 represents the path of knowledge and spiritual awakening. You are here to seek truth, both external and internal. Your analytical mind and intuition combine to pierce illusion.</string>
    <string name="life_path_7_strength_1">Deep analytical abilities</string>
    <string name="life_path_7_strength_2">Strong intuition</string>
    <string name="life_path_7_strength_3">Natural knowledge seeker</string>
    <string name="life_path_7_strength_4">Technical skill</string>
    <string name="life_path_7_strength_5">Spiritual awareness</string>
    <string name="life_path_7_challenge_1">Tendency toward isolation</string>
    <string name="life_path_7_challenge_2">Can become overly skeptical</string>
    <string name="life_path_7_challenge_3">Risk of emotional withdrawal</string>
    <string name="life_path_7_challenge_4">Difficulty trusting others</string>
    <string name="life_path_7_challenge_5">Prone to escapism</string>
    <string name="life_path_7_career_1">Scientist</string>
    <string name="life_path_7_career_2">Philosopher</string>
    <string name="life_path_7_career_3">Researcher</string>
    <string name="life_path_7_career_4">Psychologist</string>
    <string name="life_path_7_career_5">Programmer</string>
    <string name="life_path_7_career_6">Spiritual Teacher</string>
    <string name="life_path_7_relationship">You need a partner who respects your need for solitude and intellectual depth. Avoid withdrawing; vulnerability is not weakness.</string>
    <string name="life_path_7_spiritual">Your spiritual path is naturally inclined toward awareness. Balance intellectual understanding with experiential knowledge through meditation and contemplation.</string>
    <string name="life_path_7_colors">Purple, Violet, Brown</string>
    <string name="life_path_7_days">Monday, Saturday</string>

    <string name="life_path_8_title">The Power</string>
    <string name="life_path_8_meaning">Life Path 8 represents the path of material mastery and authority. You are here to learn responsible use of power, money, and influence. Your capacity for worldly success is immense.</string>
    <string name="life_path_8_strength_1">Exceptional business acumen</string>
    <string name="life_path_8_strength_2">Natural authority</string>
    <string name="life_path_8_strength_3">Strong manifestation abilities</string>
    <string name="life_path_8_strength_4">Organizational talent</string>
    <string name="life_path_8_strength_5">Resilience and determination</string>
    <string name="life_path_8_challenge_1">Tendency toward materialism</string>
    <string name="life_path_8_challenge_2">Can become ruthless</string>
    <string name="life_path_8_challenge_3">Risk of workaholism</string>
    <string name="life_path_8_challenge_4">Difficulty with emotional expression</string>
    <string name="life_path_8_challenge_5">Prone to power struggles</string>
    <string name="life_path_8_career_1">CEO</string>
