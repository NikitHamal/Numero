    <string name="life_path_8_relationship">You may struggle to balance work and relationships. Find a partner who is equally ambitious or secure enough to support your drive without feeling neglected.</string>
    <string name="life_path_8_spiritual">Your spiritual lesson is to use material success as a tool for higher purpose. True power lies in empowering others, not personal accumulation.</string>
    <string name="life_path_8_colors">Black, Dark Blue, Brown</string>
    <string name="life_path_8_days">Saturday, Thursday</string>

    <string name="life_path_9_title">The Humanitarian</string>
    <string name="life_path_9_meaning">Life Path 9 represents the path of universal love and completion. You are here to serve humanity through compassion, wisdom, and selfless giving. Your life embraces all the lessons of numbers 1-8.</string>
    <string name="life_path_9_strength_1">Deep compassion for humanity</string>
    <string name="life_path_9_strength_2">Broad perspective</string>
    <string name="life_path_9_strength_3">Natural charisma</string>
    <string name="life_path_9_strength_4">Artistic talents</string>
    <string name="life_path_9_strength_5">Wisdom and insight</string>
    <string name="life_path_9_challenge_1">Tendency toward martyrdom</string>
    <string name="life_path_9_challenge_2">May neglect personal needs</string>
    <string name="life_path_9_challenge_3">Risk of disillusionment</string>
